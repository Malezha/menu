{
    "name": "malezha/laravel-menu",
    "description": "Menu builder for Laravel 5",
    "keywords": ["menu", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Oleg Isaev",
            "email": "malezha@email.ua"
        }
    ],
    "require": {
        "illuminate/container": "^5.1",
        "illuminate/config": "^5.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.7",
        "orchestra/testbench": "~3.0"
    },
    "suggest": {
        "illuminate/view": "For using BladeRender"
    },
    "autoload": {
        "psr-4": {
            "Malezha\\Menu\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Malezha\\Menu\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
